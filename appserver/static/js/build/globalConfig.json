{
    "meta": {
        "name": "TA-plex-monitoring",
        "displayName": "Plex Monitoring",
        "version": "1.0.0",
        "restRoot": "TA_plex_monitoring",
        "schemaVersion": "0.0.3"
    },
    "pages": {
        "configuration": {
            "title": "Configuration",
            "description": "Set up your add-on",
            "tabs": [
                {
                    "name": "logging",
                    "title": "Logging",
                    "entity": [
                        {
                            "field": "loglevel",
                            "label": "Log level",
                            "type": "singleSelect",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG",
                                        "value": "DEBUG"
                                    },
                                    {
                                        "label": "INFO",
                                        "value": "INFO"
                                    },
                                    {
                                        "label": "WARNING",
                                        "value": "WARNING"
                                    },
                                    {
                                        "label": "ERROR",
                                        "value": "ERROR"
                                    },
                                    {
                                        "label": "CRITICAL",
                                        "value": "CRITICAL"
                                    }
                                ]
                            },
                            "defaultValue": "INFO"
                        }
                    ]
                },
                {
                    "name": "additional_parameters",
                    "title": "Add-on Settings",
                    "entity": [
                        {
                            "field": "unique_account_name",
                            "label": "Unique Account Name",
                            "type": "text",
                            "help": "This can be anything you like.",
                            "required": true,
                            "defaultValue": "",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "Ex. Plex_Account_1"
                            }
                        },
                        {
                            "field": "plex_server",
                            "label": "Plex Server",
                            "type": "text",
                            "help": "Hostname or IP of your Plex Server",
                            "required": true,
                            "defaultValue": "",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "Ex. plex.example.com or 10.0.0.2"
                            }
                        },
                        {
                            "field": "plex_port",
                            "label": "Plex Port",
                            "type": "text",
                            "help": "Default port is 32400 for Plex",
                            "required": true,
                            "defaultValue": "32400",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "plex_token",
                            "label": "Plex Token",
                            "type": "text",
                            "help": "More information regarding finding your token: https://www.plexopedia.com/plex-media-server/general/plex-token/",
                            "required": true,
                            "defaultValue": "",
                            "encrypted": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of password is 8192"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
